<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
  <item android:state_pressed="true">
    <shape
      android:shape="oval"
      android:useLevel="false">
      <stroke android:color="@color/primary_light_x4" android:width="1dp" />
      <solid android:color="#44ffffff" />
    </shape>
  </item>
  <item>
    <shape
      android:shape="oval"
      android:useLevel="false">
      <stroke android:color="@color/primary_light_x4" android:width="1dp" android:dashGap="4.9dp" android:dashWidth="1dp" />
    </shape>
  </item>
</selector>
